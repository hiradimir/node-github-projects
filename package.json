{
  "name": "node-github-projects",
  "version": "0.0.2",
  "private": false,
  "main": "dist/js/main/index.js",
  "typings": "dist/js/main/index.d.ts",
  "scripts": {
    "clean": "rimraf dist/",
    "start": "node ./bin/www",
    "tsc": "tsc",
    "test": "NODE_ENV=test; mocha --opts src/spec/mocha.opts",
    "test-debug": "NODE_ENV=test; mocha --inspect --debug-brk --opts src/spec/mocha.opts",
    "ncu": "ncu",
    "prepublish": "npm run build && npm test",
    "prebuild": "npm run clean",
    "build": "npm run tsc",
    "coverage": "npm run build && NODE_ENV=test; istanbul cover --print none -x 'dist/js/spec/**/*.js' --dir dist/coverage/ _mocha -- --opts test/mocha.opts",
    "postcoverage": "remap-istanbul -i dist/coverage/coverage.json -o dist/coverage/ts/html-report -t html && remap-istanbul -i dist/coverage/coverage.json -t text -o dist/coverage/ts/coverage.text; cat dist/coverage/ts/coverage.text && remap-istanbul -i dist/coverage/coverage.json -o dist/coverage/ts/cobertura.xml -t cobertura"
  },
  "directories": {
    "test": "src/spec"
  },
  "dependencies": {
    "bluebird": "^3.5.0",
    "dotenv": "^4.0.0",
    "github": "^9.2.0",
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.5",
    "@types/github": "7.1.0",
    "@types/lodash": "^4.14.64",
    "@types/mocha": "^2.2.41",
    "@types/node": "^7.0.23",
    "@types/power-assert": "^1.4.29",
    "espower-typescript": "^8.0.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.4.2",
    "npm-check-updates": "^2.11.2",
    "power-assert": "^1.4.2",
    "remap-istanbul": "^0.9.5",
    "rimraf": "^2.6.1",
    "typescript": "^2.3.4",
    "typescript-simple": "https://github.com/hiradimir/typescript-simple.git#dummy-publishing"
  }
}
